{
  "name": "dark-grey/esia",
  "license": "MIT",
  "description": "OpenID ESIA authenticating",
  "keywords": [
    "esia",
    "openid",
    "egov"
  ],
  "autoload": {
    "psr-4": {
      "Esia\\": "src/Esia"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\" : "tests"
    }
  },
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "^6.1.0",
    "psr/log": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-client": "^1.0",
    "ext-json": "*",
  },
  "suggest": {
    "ext-openssl": "SignerPKCS7 support"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "codeception/codeception": "^2.4"
  }
}
